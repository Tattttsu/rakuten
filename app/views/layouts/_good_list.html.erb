
<% @goods.each do |item| %>
　<div class="goods-list">
    <div class="goods-img">
    　<li><img src="<%= item['mediumImageUrls'][0] %>"></li>
    </div>
    <div class="goods-list-right">
      <div class="goods-name">
        <li><a href="<%= item['itemUrl'] %>" target="_blank"><%= item['itemName'] %></a></li>
      </div>
      <div class="goods-price">
        <li><%= item['itemPrice'] %>円</li>
        <li><%= item["itemCode"] %></li>
        <li><%= item %></li>
      </div>
    </div>
  </div>
  

<%= link_to "お気に入り登録",goods_path(itemCode: item["itemCode"]), method: :post %>
<%= link_to "削除", delete_path(itemCode: item["itemCode"]), method: :delete,data: { confirm: "本当に削除しますか?" } %>
<%= item["itemCode"] %>
  <%= debug(params) if Rails.env.development? %>
<% end %>

 